export fn test_shake_imem_avx2x4
( reg mut ptr u256[25] st
, reg u64 obuf0 obuf1 obuf2 obuf3 inbuf
) -> reg ptr u256[25]
{
  st = __state_init_avx2x4(st);
  st, _, _ = __absorb_bcast_imem_avx2x4(st, 0, inbuf, BUFLEN, R136, SHAKE);
  _, _, _, _, st = __squeeze_imem_avx2x4(obuf0, obuf1, obuf2, obuf3, OUTLEN, st, R136);
  return st;
}

